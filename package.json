{
  "name": "semterface",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "git://github.com/SEMterface/setterface.git"
  },
  "bin": "./bin/www",
  "description": "A web application for controlling microscopes.",
  "main": "index.js",
  "scripts": {
    "start": "node ./bin/www",
    "test": "npm run tape | tap-spec && standard | snazzy",
    "cover": "istanbul cover tape test/*.js| tap-spec && standard | snazzy",
    "submitCover": "cat coverage/lcov.info | codeclimate",
    "travis": "npm run cover && npm run submitCover"
  },
  "dependencies": {
    "body-parser": "~1.13.2",
    "cookie-parser": "~1.3.5",
    "debug": "~2.2.0",
    "express": "~4.13.1",
    "jade": "~1.11.0",
    "morgan": "~1.6.1",
    "persona-buttons": "^1.0.1",
    "request": "^2.48.0",
    "serve-favicon": "~2.3.0",
    "socket.io": "^1.3.3",
    "socket.io-client": "^1.3.3"
  },
  "devDependencies": {
    "codeclimate-test-reporter": "^0.0.4",
    "istanbul": "^0.3.0",
    "nodemon": "^1.3.6",
    "tap-spec": "^2.1.2",
    "tape": "^3.0.3",
    "standard": "^5.4.1",
    "snazzy": "^2.0.1",
    "standard-format": "^2.1.0"
  },
  "author": {
    "name": "Bret Comnes",
    "url": "http://bret.io",
    "email": "bcomnes@pdx.edu"
  },
  "contributors": [
    {
      "name": "Galen Gledhil",
      "email": "ggalen@pdx.edu"
    }
  ],
  "homepage": "https://github.com/SEMterface/semterface",
  "bugs": "http://github.com/SEMterface/semterface/issues",
  "license": "MIT",
  "jshintConfig": {
    "node": true,
    "jquery": false,
    "strict": false,
    "globalstrict": false,
    "asi": true
  }
}
