{
  "name": "semterface",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "git://github.com/SEMterface/setterface.git"
  },
  "bin": "./bin/www",
  "description": "A web application for controlling microscopes.",
  "main": "index.js",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "tape": "tape test/*.js",
    "test": "npm run tape | tap-spec",
    "cover": "istanbul cover tape test/*.js| tap-spec",
    "submitCover": "cat coverage/lcov.info | codeclimate",
    "travis": "npm run cover && npm run submitCover",
    "mon": "nodemon -i coverage --exec 'npm --silent start'",
    "tester": "nodemon -i coverage --exec 'npm --silent run cover'"
  },
  "dependencies": {
    "body-parser": "^1.8.1",
    "browserify-middleware": "^4.1.0",
    "cookie-parser": "^1.3.3",
    "debug": "^2.0.0",
    "express": "^4.9.0",
    "express-autoprefixer": "^3.0.0",
    "jade": "^1.9.1",
    "jquery": "^2.1.1",
    "minimist": "^1.1.0",
    "morgan": "^1.3.0",
    "request": "^2.48.0",
    "serve-favicon": "^2.1.3",
    "simplewebrtc": "^1.11.1",
    "socket.io": "^1.2.0",
    "socket.io-client": "^1.2.1"
  },
  "devDependencies": {
    "browserify": "^6.3.2",
    "codeclimate-test-reporter": "^0.0.3",
    "istanbul": "^0.3.0",
    "nodemon": "^1.3.6",
    "tap-spec": "^2.1.2",
    "tape": "^3.0.3"
  },
  "engines": {
    "node": "0.10.x",
    "npm": "2.1.x"
  },
  "author": {
    "name": "Bret Comnes",
    "url": "http://bret.io",
    "email": "bcomnes@pdx.edu"
  },
  "contributors": [
    {
      "name": "Galen Gledhil",
      "email": "ggalen@pdx.edu"
    }
  ],
  "directories": {
    "lib": "./lib",
    "bin": "./bin"
  },
  "homepage": "https://github.com/SEMterface/semterface",
  "bugs": "http://github.com/SEMterface/semterface/issues",
  "license": "MIT"
}
